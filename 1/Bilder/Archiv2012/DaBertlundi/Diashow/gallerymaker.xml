<?xml version='1.0' encoding='UTF-8'?>
<galleryMaker xmlns="http://ns.adobe.com/AMG/" >
    <galleryInfo>
        <galleryName>Superstar</galleryName>
        <galleryDescription></galleryDescription>
        <creator designer="James Karsberg, Szczepan Waclaw, James Laver and Paul Cummins" company="Lightmaker" ></creator>
        <category>Interactive</category>
        <thumbnailPath src="gallerythumb.jpg" ></thumbnailPath>
        <windowSize width="-1024" height="-575" ></windowSize>
        <formatsSupported>images</formatsSupported>
    </galleryInfo>
    <sizes>
        <size width="260" height="260" name="thumb" ></size>
        <size width="1030" height="1030" name="large" ></size>
        <size width="640" height="480" name="video" ></size>
    </sizes>
    <!-- Gallery Settings -->
    <gallerySettings>
        <setting id="GroupsPath" >resources/groups/group.xml</setting>
        <!-- <setting id="StylesPath">resources/styles/style.xml</setting> -->
    </gallerySettings>
    <!-- Interface Labels -->
    <galleryInterface>
        <label id="GalleryTitle" >Galerietitel</label>
        <label id="GalleryColor" >Farbe</label>
        <label id="MusicVolume" >Musiklautstärke</label>
        <label id="GallerySave" >Galerie speichern</label>
        <label id="GalleryTint" >Hintergrundfarbstärke</label>
        <label id="TitlePane" >Titel</label>
        <label id="ColourPane" >Farbe</label>
        <label id="SavePane" >Speichern</label>
        <label id="TXT_HIDE" >Details ausblenden</label>
        <label id="TXT_SHOW" >Details einblenden</label>
        <label id="SOUND_ON" >Stumm deakt.</label>
        <label id="SOUND_OFF" >Stumm</label>
        <label id="AUTO_PLAY" >Abspielen</label>
        <label id="AUTO_STOP" >Anhalten</label>
        <label id="SoundPane" >Geräusche</label>
    </galleryInterface>
</galleryMaker>
